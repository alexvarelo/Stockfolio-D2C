{
  "version": 2,
  "routes": [
    {
      "src": "/(.*)\\.(ico|png|jpg|jpeg|svg|webp|gif)",
      "headers": {
        "cache-control": "public, max-age=0, must-revalidate"
      }
    },
    { "handle": "filesystem" },
    { "src": "/.*", "dest": "/index.html" }
  ],
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ]
}
